    "ejs": "^3.0.2",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^8.0.8",
    "koa-views": "^6.2.1",
    "log4js": "^6.1.2",
    "mongoose": "^5.9.7",
    "nodemon": "^2.0.2"
  }
}
